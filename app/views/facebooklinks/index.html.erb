<p id="notice"><%= notice %></p>

<h1>Listing Facebooklinks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @facebooklinks.each do |facebooklink| %>
      <tr>
        <td><%= facebooklink.name %></td>
        <td><%= facebooklink.url %></td>
        <td><%= link_to 'Show', facebooklink %></td>
        <td><%= link_to 'Edit', edit_facebooklink_path(facebooklink) %></td>
        <td><%= link_to 'Destroy', facebooklink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facebooklink', new_facebooklink_path %>
